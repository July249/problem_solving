   "source": [
    "def digit_reverse(n):\n",
    "    # 함수를 완성해 주세요\n",
    "    ret =[]\n",
    "    for i in str(n):\n",
    "        ret.append(int(i))\n",
    "    ret.reverse() \n",
    "    return ret\n",
    "\n",
    "# 아래는 테스트로 출력해 보기 위한 코드입니다.\n",
    "print(\"결과 : {}\".format(digit_reverse(12345)));"
   ]
  }
